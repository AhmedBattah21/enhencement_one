<StackPane prefHeight="424.0" prefWidth="375.0" stylesheets="@../AppCss/studentregistration_stepone.css" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="reportgenthree.views.StudentRegistration_steponeController">
   <children>
      <AnchorPane id="AnchorPane" maxHeight="420.0" maxWidth="370.0" prefHeight="420.0" prefWidth="370.0" styleClass="infor_layout">
          <stylesheets>
              <URL value="@/reportgenthree/AppCss/studentregistration_stepone.css" />
          </stylesheets>
         <children>
            <VBox prefHeight="400.0" prefWidth="367.0" spacing="15.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <HBox alignment="CENTER" prefHeight="36.0" prefWidth="337.0">
                     <children>
                        <Label prefHeight="17.0" prefWidth="221.0" text="Complete Step One To Access Step Two">
                           <font>
                              <Font name="Bodoni MT Italic" size="13.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="52.0" prefWidth="337.0" styleClass="infor_hbox">

                           <font>
                              <Font size="15.0" />
                           </font>
                        </TextField>
                        <Label fx:id="infor1" contentDisplay="GRAPHIC_ONLY" prefHeight="35.0" prefWidth="20.0" text="Label" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" layoutX="10.0" layoutY="46.0" prefHeight="52.0" prefWidth="337.0" styleClass="infor_hbox">
                     <children>
                        <Label prefHeight="35.0" prefWidth="68.0" text="Last Name:" />
                        <TextField fx:id="txt_lname" onKeyReleased="#keylistener_lname" prefHeight="35.0" prefWidth="172.0" promptText="Last Name(s)" HBox.hgrow="ALWAYS">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </TextField>
                        <Label fx:id="infor2" contentDisplay="GRAPHIC_ONLY" prefHeight="35.0" prefWidth="20.0" text="Label" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="108.0" prefHeight="52.0" prefWidth="337.0" styleClass="infor_hbox">
                     <children>
                        <Label prefHeight="35.0" prefWidth="68.0" text="Gender:" />
                        <ComboBox fx:id="cmb_gender" onKeyReleased="#keylistener_gender" prefHeight="35.0" prefWidth="150.0" promptText="Select Gender" />
                        <Label fx:id="infor3" contentDisplay="GRAPHIC_ONLY" prefHeight="35.0" prefWidth="20.0" text="Label" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="165.0" prefHeight="52.0" prefWidth="337.0" styleClass="infor_hbox">
                     <children>
                        <Label prefHeight="35.0" prefWidth="68.0" text="Study Year:" />
                        <ComboBox fx:id="cmb_stdyear" onKeyReleased="#keylistener_stdyear" prefHeight="35.0" prefWidth="150.0" promptText="Select Year" />
                        <Label fx:id="infor4" contentDisplay="GRAPHIC_ONLY" prefHeight="35.0" prefWidth="20.0" text="Label" />

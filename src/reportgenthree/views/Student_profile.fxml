<StackPane prefHeight="456.0" prefWidth="765.0" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="reportgenthree.views.Student_profileController">
   <children>
      <BorderPane prefHeight="500.0" prefWidth="700.0">

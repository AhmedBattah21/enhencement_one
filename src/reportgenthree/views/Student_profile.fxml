            <HBox alignment="CENTER_RIGHT" prefHeight="40.0" prefWidth="765.0" BorderPane.alignment="CENTER">
               <children>
                  <HBox alignment="CENTER_RIGHT" maxWidth="200.0" prefHeight="40.0" prefWidth="200.0" spacing="3.0">
